{
	"configs": {
		"application": {
			"name": "application",
			"tdg": {
				"id_fields": {
					"application_id": {
						"regex": {
							"exp": "[1-9]{1,3}-[a-z]{3,8}",
							"case_sensitive": true
						},
						"unique": true
					}
				}
			}
		},
		"account": {
			"name": "account",
			"tdg": {
				"id_fields": {
					"account_id": {
						"faker": "finance.account",
						"unique": true,
						"elements_per_source": true
					}
				}
			}
		},
		"identity": {
			"name": "identity",
			"tdg": {
				"id_fields": {
					"user_id": {
						"faker": "finance.account",
						"unique": true
					},
					"uid2": {
						"faker": "finance.account",
						"unique": true
					},
					"mail": {
						"tdg": {
							"function": "email",
							"domainName": "gumbo.com"
						},
						"unique": true
					},
					"fn": {
						"tdg": "first_name"
					},
					"ln": {
						"tdg": "last_name"
					}
				}
			}
		}
	}
}
