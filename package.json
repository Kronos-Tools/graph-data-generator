  "engines": {
    "node": ">=5"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "istanbul": "0.4.2",
    "jsdoc": "3.4.0",
    "mocha": "2.4.5",
    "rimraf": "^2.5.1",
    "semantic-release": "6.2.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
